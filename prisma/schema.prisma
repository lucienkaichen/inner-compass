generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Entry {
  id        Int      @id @default(autoincrement())
  content   String
  mood      String? 
  tags      String?  
  createdAt DateTime @default(now())
}

model Strategy {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  tags      String?
  createdAt DateTime @default(now())
}

